    <span class="s">"whoswho"</span><span class="p">:</span> <span class="n">ids_whoswho</span><span class="p">,</span>
    <span class="s">"mortality"</span><span class="p">:</span> <span class="n">ids_mortality</span>
<span class="p">}</span>

